   "source": [
    "n = int(input(\"Enter the number: \"))\n",
    "factorial = 1\n",
    "\n",
    "if n==0 or n==1:\n",
    "    print(\"Factorial is 1\")\n",
    "elif n<0:\n",
    "    print(\"Factorial of negative number is not defined\")\n",
    "else:\n",
    "    for i in range(1, n+1):\n",
    "        factorial *= i\n",
    "    print(\"Factorial of\",n, \"is\",factorial)\n",
    "    "
