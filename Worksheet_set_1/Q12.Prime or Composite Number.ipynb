   "source": [
    "n=int(input(\"Enter a number: \"))\n",
    "\n",
    "if n>1:\n",
    "    for i in range(2, n):\n",
    "        if n % i ==0:\n",
    "            print(n, \"is a composite number\")\n",
    "            break\n",
    "    else:\n",
    "        print(n, \"is a prime number\")\n",
    "elif n==1 or n==0:\n",
    "    print(n,\"is neither prime nor composite\")\n",
